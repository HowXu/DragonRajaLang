{
  "name": "dragonrajalang",
  "displayName": "DragonRajaLang",
  "description": "Personal extension for my dragon raja fiction fan writing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "auto-tab-on-enter.onEnter",
        "title": "Auto Tab on Enter"
      }
    ],
    "keybindings": [
      {
        "command": "auto-tab-on-enter.onEnter",
        "key": "enter",
        "when": "editorTextFocus"
      }
    ],
    "languages": [
      {
        "id": "dr",
        "aliases": [
          "DR Document",
          "dr"
        ],
        "extensions": [
          ".dr"
        ]
      }
    ],
    "grammars": [
      {
        "language": "dr",
        "scopeName": "source.dr",
        "path": "./syntaxes/dr.lang.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
